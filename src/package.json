{
  "private": true,
  "name": "now-site",
  "version": "1.0.0",
  "description": "",
  "main": "start.js",
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "prepush": "npm run lint:scss && npm test",
    "clean": "rimraf dist/ coverage/ .build/ && mkdirp dist/scripts dist/styles .build/",
    "start": "node ./start.js",
    "test": "cross-env APP_UNIT_TEST=true babel-node ./node_modules/babel-istanbul/lib/cli cover --root ./ --include-all-sources --report teamcity --report cobertura --report html ./node_modules/mocha/bin/_mocha -- --require ./test/setup.js --recursive --check-leaks",
    "test:tdd": "mocha --require babel-polyfill --require babel-register --recursive --check-leaks --watch",
    "build": "npm run clean && npm run build:css && npm run build:js",
    "build:css": "npm run build:css:site && npm run build:css:styleguide",
    "build:css:site": "node-sass --output-style compressed app/styles/main.scss > .build/_main.css && postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions, ie >= 10, ios >= 7, Android >= 4\" --output dist/styles/main.css .build/_main.css",
    "build:css:styleguide": "node-sass --output-style compressed app/styles/styleguide.scss > .build/_styleguide.css && postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions, ie >= 10, ios >= 7, Android >= 4\" --output dist/styles/styleguide.css .build/_styleguide.css",
    "build:js": "browserify app/client.js --debug --transform babelify --transform [envify --APP_KEY $APP_KEY] --plugin [minifyify --output ./dist/bundle.map] --ignore-missing --external request --outfile dist/scripts/main.js",
    "predev": "npm run build",
    "dev": "npm run dev:css & npm run dev:js & npm run dev:serve",
    "dev-win": "npm run build:css && npm run build:js && npm start",
    "dev:css": "nodemon --watch app/styles --watch app-$APP_KEY/styles --ext scss --exec \"npm run build:css\"",
    "dev:js": "watchify app/client.js --debug --transform babelify --transform [envify --APP_KEY $APP_KEY] --plugin [minifyify --output ./dist/bundle.map] --ignore-missing --external request --outfile dist/scripts/main.js",
    "dev:serve": "cross-env BABEL_DISABLE_CACHE=1 nodemon --watch app start.js",
    "stubbed": "cross-env NODE_ENV=stubbed npm run dev",
    "lint:scss": "sass-lint --config .sass-lint.yml --ignore './**/vendor/**' --no-exit --verbose './app*/styles/**/*.scss'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bauerxcelmedia/now-site.git"
  },
  "author": "",
  "license": "",
  "homepage": "https://github.com/bauerxcelmedia/now-site#readme",
  "dependencies": {
    "@bxm/ad": "^9.0.1",
    "@bxm/article": "^17.3.0",
    "@bxm/behaviour": "^1.2.0",
    "@bxm/config": "^4.1.1",
    "@bxm/datetime": "^2.0.0",
    "@bxm/flux": "^4.2.0",
    "@bxm/gallery": "^13.0.0",
    "@bxm/header": "^11.0.1",
    "@bxm/markdown": "^1.2.0",
    "@bxm/modal": "^2.1.0",
    "@bxm/nav": "^3.1.1",
    "@bxm/newsletter": "^3.0.0",
    "@bxm/node-apm": "^1.0.1",
    "@bxm/react-script-loader": "^2.1.0",
    "@bxm/recommendations": "^4.1.0",
    "@bxm/server": "^16.5.3",
    "@bxm/site-header": "^2.0.1",
    "@bxm/social": "^1.3.0",
    "@bxm/tags": "^2.2.0",
    "@bxm/teaser": "^4.5.0",
    "@bxm/ui": "^15.0.1",
    "@bxm/video": "^4.0.0",
    "@bxm/winston-logger": "^1.1.0",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.6.5",
    "classnames": "^2.1.2",
    "console-shim": "^1.0.3",
    "cross-env": "^1.0.7",
    "exenv": "^1.2.0",
    "express": "^4.13.3",
    "fluxible": "^1.0.3",
    "fluxible-addons-react": "^0.2.2",
    "fluxible-reducer-store": "^0.1.0",
    "fluxible-router": "^0.4.11",
    "lodash": "^3.10.1",
    "lru-cache": "^4.0.1",
    "picturefill": "^3.0.1",
    "promise": "^7.1.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-inlinesvg": "^0.5.3",
    "request": "^2.67.0",
    "superagent": "^1.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.5.1",
    "babel-istanbul": "^0.7.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-optional": "^1.0.0",
    "chai": "^3.5.0",
    "enzyme": "^2.5.1",
    "husky": "^0.11.4",
    "jsdom": "^8.0.2",
    "minifyify": "^7.3.2",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "postcss-cli": "^2.5.1",
    "proxyquire": "^1.7.3",
    "rimraf": "^2.5.2",
    "sass-lint": "^1.7.0",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "watchify": "^3.7.0"
  }
}

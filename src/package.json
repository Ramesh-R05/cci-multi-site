{
    "private": true,
    "name": "multi-site",
    "version": "1.0.0",
    "description": "",
    "main": "start.js",
    "engines": {
        "node": ">=8.9.0",
        "npm": ">=5.5.0"
    },
    "scripts": {
        "prepush": "run-p lint test",
        "start": "NODE_ENV=production node ./start.js",
        "test": "nyc --include-all-sources --report-dir ../shippable/codecoverage -r=cobertura -r=html mocha --require @babel/polyfill --require @babel/register --require ./test/setup.js --recursive --check-leaks",
        "test:debug": "nyc --include-all-sources -r=cobertura -r=html -r=text -r=html mocha --require ./test/setup.js --recursive --check-leaks",
        "test:ci": "npm run test -- --reporter=xunit --reporter-options output=./../shippable/testresults/result.xml",
        "test:unit": "mocha --require @babel/polyfill --require @babel/register --require ./test/setup.js --reporter min --recursive --check-leaks",
        "test:tdd": "npm run test:unit -- --watch",
        "clean": "rimraf dist/ coverage/ && mkdir dist",
        "build": "npm run clean && NODE_ENV=production webpack --bail --hide-modules --progress --mode production",
        "dev": "npm run clean && webpack --bail --debug --display-error-details --hide-modules --progress --watch --mode development & npm run serve",
        "dev:debug": "npm run clean && webpack --bail --debug --display-error-details --hide-modules --progress --watch --mode development & npm run serve:debug",
        "dev:prod": "npm run build && npm run start",
        "dev:win": "npm run clean && webpack --bail --debug --display-error-details --hide-modules --progress && npm run serve",
        "serve": "nodemon --watch app --ext js ./start.js",
        "serve:debug": "nodemon $NODE_DEBUG_OPTION --watch app --ext js ./start.js",
        "lint:scss": "sass-lint-auto-fix --config .sass-lint.yml './app*/styles/**/*.scss'",
        "fmt:js": "prettier --write './*.js' 'app/**/*.js' 'app-*/**/*.js' 'test/**/*.js' 'automation/**/*.js'",
        "lint:js": "eslint --fix --ext .js ./ app/ app-*/",
        "lint": "run-s lint:scss fmt:js lint:js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bauerxcelmedia/multi-site.git"
    },
    "author": "",
    "license": "",
    "homepage": "https://github.com/bauerxcelmedia/multi-site#readme",
